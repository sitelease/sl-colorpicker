{
    "name": "sitelease/sl-palette-color-field",
    "description": "Color picker fields built in react",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.1",
        "sitelease/sl-open-core": "*",
        "sitelease/sl-live-color-field": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Sitelease\\PaletteColorField\\": "src/",
            "Sitelease\\PaletteColorField\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ],
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
